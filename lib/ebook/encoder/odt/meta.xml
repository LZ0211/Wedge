<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<office:document-meta xmlns:office="urn:oasis:names:tc:opendocument:xmlns:office:1.0" xmlns:meta="urn:oasis:names:tc:opendocument:xmlns:meta:1.0" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:xlink="http://www.w3.org/1999/xlink" office:version="1.2">
	<office:meta>
		<meta:generator>MicrosoftOffice/15.0 MicrosoftWord</meta:generator>
		<dc:title>{{=book.meta.title}}</dc:title>
		<dc:description>{{=book.meta.brief}}</dc:description>
		<dc:subject>{{=book.meta.classes}}</dc:subject>
		<meta:initial-creator>{{=book.meta.author}}</meta:initial-creator>
		<dc:creator>{{=book.meta.author}}</dc:creator>
		<meta:creation-date>{{=(new Date(book.meta.date)).formation('yyyy-MM-ddThh:mm:ssZ')}}</meta:creation-date>
		<dc:date>{{=(new Date(book.meta.date)).formation('yyyy-MM-ddThh:mm:ssZ')}}</dc:date>
	</office:meta>
</office:document-meta>