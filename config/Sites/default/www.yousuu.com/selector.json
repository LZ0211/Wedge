{
  "infoPage": {
    "match": "/book\\/\\d+$/i.test($.location())",
    "indexPage": "$.location()",
    "footer": "$('.footer').length",
    "bookInfos": {
      "origin": "$.location()",
      "title": "$('h1.book-name').eq(0).text()",
      "author": "$('.book-author').text()",
      "classes": "$('.el-tag a').last().text()",
      "isend": "$('.status').text()",
      "cover": "$.location($('.card-bookInfo-cover').attr('cover'))",
      "brief": "$.raw.split('introduction:\"')[1].split('\",countWord')[0]"
    }
  }
}