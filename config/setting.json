  "request": {
    "reconnect": 5,
    "imageReconnect": 3,
    "timeout": 5000,
    "proxy": false,
    "proxyAuth": "",
    "proxyAutoConfig": ""
  },
  "ebook": {
    "activated": -1,
    "formation": "epub",
    "filename": "{author}-《{title}》.{format}",
    "directory": "../ebook",
    "filter": "content.length:<500,>",
    "openDirectory": false,
    "openEbookFile": false
  },
  "thread": {
    "execute": 10,
    "refresh": 8,
    "new": 3,
    "update": 3,
    "image": 2
  },
  "book": {
    "sync": true,
    "changeSource": false,
    "override": true,
    "imageLocalization": false,
    "searchMeta": true,
    "deepDownload": false,
    "maxDepth": 0,
    "localization": true,
    "imageExts": [".jpg",".jpeg",".png",".gif",".tif",".webp",".bmp",".tga"],
    "imageMinSize": 5000,
    "unique":{
      "source":true,
      "id":true,
      "title":false
    }
  },
  "database": {
    "sync": true,
    "check": false,
    "primary": "uuid",
    "showKeys": ["uuid","title","author","classes","isend","date","source"]
  },
  "app": {
    "log": true,
    "debug": false,
    "info": true,
    "retry": {
      "meta": 3,
      "index": 3,
      "cover": 3,
      "chapter": 3,
      "image": 3
    }
  }
